{
  "$schema-template": "1.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "containerRegistry": {
                "username": "$CONTAINER_REGISTRY_USERNAME",
                "password": "$CONTAINER_REGISTRY_PASSWORD",
                "address": "$CONTAINER_REGISTRY_NAME"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "EdgeStreamAnalyticsJob": {
            "settings": {
                "image": "mcr.microsoft.com/azure-stream-analytics/azureiotedge:1.0.7",
                "createOptions": ""
            },
            "type": "docker",
            "version": "1.0",
            "env": {
                "PlanId": {
                    "value": "stream-analytics-on-iot-edge"
                }
            },
            "status": "running",
            "restartPolicy": "always"
         },
          "AIVisionDevKitGetStartedModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.AIVisionDevKitGetStartedModule.arm32v7}",
              "createOptions": {
                "HostConfig": {
                  "NetworkMode": "host",
                  "Binds": [
                    "/data/misc/camera:/app/vam_model_folder",
                    "/run/systemd:/run/systemd"
                  ]
                },
                "NetworkingConfig": {
                  "EndpointsConfig": {
                    "host": {}
                  }
                }
              }
            }
          },
          "WebStreamModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.WebStreamModule}",
              "createOptions": {
                "ExposedPorts": {
                  "3000/tcp": {},
                  "3002/tcp": {}
                },
                "HostConfig": {
                  "NetworkMode": "host",
                  "PortBindings": {
                    "3000/tcp": [
                      {
                        "HostPort": "3000"
                      }
                    ],
                    "3002/tcp": [
                      {
                        "HostPort": "3002"
                      }
                    ]
                  }
                },
                "NetworkingConfig": {
                  "EndpointsConfig": {
                    "host": {}
                  }
                }
              }
            },
            "env": {
              "RTSP_IP": {
                "value": "localhost"
              },
              "RTSP_PORT": {
                "value": "8900"
              },
              "RTSP_PATH": {
                "value": "live"
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "AIVisionDevKitGetStartedModuleToEdgeStreamAnaltyticsJob": "FROM /messages/modules/AIVisionDevKitGetStartedModule/outputs/* INTO BrokeredEndpoint(\"/modules/EdgeStreamAnalyticsJob/inputs/devkitinput\")",
          "EdgeStreamAnaltyticsJobToUpstream": "FROM /messages/modules/EdgeStreamAnalyticsJob/* INTO $upstream"
       },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "AIVisionDevKitGetStartedModule": {
      "properties.desired": {
        "ModelZipUrl": "",
        "TimeBetweenMessagesInSeconds": 1,
        "ObjectsOfInterest": "[\"ALL\"]",
        "ShowVideoOverlay": true,
        "Bitrate": "1.5Mbps",
        "Resolution": "1080P",
        "VideoOverlayConfig": "inference",
        "FrameRate": 30,
        "HdmiDisplayActive": true,
        "VideoAnalyticsEnabled": true,
        "ShowVideoPreview": true,
        "Codec": "AVC/H.264"
      }
    },
    "EdgeStreamAnalyticsJob": {
      "properties.desired": {
          "ASAJobInfo": "$ASA_BLOB_URL",
          "ASAJobResourceId": "/subscriptions/a98dfc86-277a-4bed-93fa-b07419a9ddae/resourceGroups/ai-on-the-edge/providers/Microsoft.StreamAnalytics/streamingjobs/EdgeStreamAnalyticsJob",
          "ASAJobEtag": "42244656-f3db-4d16-83cd-8233477918aa",
          "PublishTimestamp": "2/3/2020 11:12:38 PM"
      }
    }
  }
}
